{
    "name": "instacar/intelimotor-api-client",
    "description": "An API Client to talk with the Intelimotor API",
    "type": "library",
    "license": "GNU LGPLv3",
    "autoload": {
        "psr-4": {
            "InstacarMX\\IntelimotorApiClient\\": "src/",
            "InstacarMX\\IntelimotorApiClient\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Brandon Antonio Lorenzo",
            "email": "bantonio@gtglobal.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "doctrine/collections": "^1.6",
        "symfony/serializer": "^4.4|^5.0",
        "symfony/http-client-contracts": "^v2.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/http-client": "^4.4|^5.0",
        "symfony/property-access": "^4.4|^5.0",
        "doctrine/annotations": "^1.13",
        "phpstan/phpstan": "^0.12.97"
    },
    "replace": {
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*"
    }
}
